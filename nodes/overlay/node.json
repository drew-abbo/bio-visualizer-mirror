{
    "name": "Overlay",
    "inputs": [
        {
            "name": "background",
            "kind": "Frame"
        },
        {
            "name": "foreground", 
            "kind": "Frame"
        },
        {
            "name": "opacity",
            "kind": {
                "Float": {
                    "default": 0.5,
                    "min": 0.0,
                    "max": 1.0
                }
            }
        }
    ],
    "outputs": [
        {
            "name": "output",
            "kind": "Frame"
        }
    ],
    "executor": {
        "Shader": {
            "source": "shader.wgsl"
        }
    }
}